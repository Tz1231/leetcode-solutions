class Solution {
    public int findNumbers(int[] nums) {
        int times ;
        int output = 0;
        for (int i = 0; i<nums.length ; i++){
            times = 1 ;
            while(nums[i]>=10){
                nums[i]=nums[i]/10;
                times++;
            }
            if(times%2 == 0){
                output++;
            }
        }
        return output;
    }
}
